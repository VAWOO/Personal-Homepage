<mapper namespace="com.board.mapper.ContactMapper">

	<sql id="ContactColumns">
		  idx
		, contact_name
		, contact_mail
		, contact_number
		, contact_message
		, insert_time
	</sql>
	
	<insert id="insertContact" parameterType="ContactDTO">
		INSERT INTO member (<include refid="ContactColumns" />)
		VALUES
		(
			  #{idx}
			, #{contactName}
			, #{contactMail}
			, #{contactNumber}
			, #{contactMessage}
			, NOW()
		)
	</insert>
